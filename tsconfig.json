{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api/*": ["src/api/*"],
      "@assets/*": ["src/assets/*"],
      "@config/*": ["src/config/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@context/*": ["src/context/*"],
      "@features/*": ["src/features/*"],
      "@hooks/*": ["src/hooks/*"],
      "@redux/*": ["src/redux/*"],
      "@routers/*": ["src/routers/*"],
      "@services/*": ["src/services/*"],
      "@styles/*": ["src/styles/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@locales/*": ["src/locales/*"],
      "@pages/*": ["src/pages/*"],
      "@/*": ["src/*"]
    }
  }
}
